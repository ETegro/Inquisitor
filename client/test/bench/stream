#!/bin/sh -f
# NAME=stream
# DESCRIPTION=STREAM memory throughput benchmark
# DESTROYS_HDD=N
# POWEROFF_DURING_TEST=N
# VERSION=0.1

. /usr/share/inquisitor/functions-test

test_started 1

RESULT=`stream-mem | grep -A 4 Function | perl -e 'while(<>){next if /^Function/;@d=split(" ",$_);$l.=$d[1].",";};chop $l;print $l;'`

test_succeeded "result=${RESULT}"
